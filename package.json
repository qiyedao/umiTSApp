    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.38.9",
    "enquire-js": "^0.2.1",
    "mockjs": "^1.1.0",
    "rc-animate": "^3.1.1",
    "rc-banner-anim": "^2.4.5",
    "rc-drawer": "^4.4.3",
    "rc-editor-list": "^2.0.1",
    "rc-queue-anim": "^2.0.0",
    "rc-scroll-anim": "^2.7.6",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^3.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "umi": "^3.5.26"
  },
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/plugin-qiankun": "^2.39.2",
    "@umijs/preset-react": "2.x",
    "@umijs/test": "^3.5.26",
    "express": "^4.18.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3",
    "yorkie": "^2.0.0"
  }
}
